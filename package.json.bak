{
  "name": "costing",
  "productName": "Costing",
  "version": "0.15.0",
  "main": "main.js",
  "//homepage": "demo/calculator/",
  "proxy": "http://localhost:8787",
  "private": true,
  "build": {
    "extends": null,
    "appId": "com.yantra.costing",
    "copyright": "Copyright Â© 2020 Yantra",
    "nsis": {
      "oneClick": false,
      "perMachine": true
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "app.ico"
    },
    "files": [
      "*.js",
      "./build/**/*",
      "./db/**/*",
      "./routers/**/*",
      "./src/utils.js"
    ]
  },
  "dependencies": {
    "@react-pdf/renderer": "^2.1.1",
    "cors": "^2.8.5",
    "decimal.js": "^10.3.1",
    "express": "^4.17.2",
    "express-handlebars": "^5.3.0",
    "get-port": "^5.1.1",
    "lodash": "^4.17.21",
    "sequelize": "^6.6.2",
    "sqlite3": "^5.0.2",
    "systeminformation": "^5.6.12",
    "umzug": "^2.3.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:electron": "yarn run electron .",
    "pack": "electron-builder --win",
    "dist": "yarn run build && yarn run pack",
    "rebuild": "yarn run electron-rebuild -f -w",
    "postinstall": "electron-builder install-app-deps",
    "install:prod": "yarn install --production=true",
	"heroku-postbuild": "yarn install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "engines": {
        "node": "14.x"
      },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
